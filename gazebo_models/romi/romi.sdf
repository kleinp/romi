<?xml version="1.0" ?>

<sdf version="1.5">
    <model name="romi">

        <plugin name="romi_control" filename="libromi_plugin.so">
        </plugin>

        <link name="chassis">
            <collision name="chassis_collision">
                <pose>0 0 .04 0 0 1.5708</pose>
                <geometry>
                    <cylinder>
                        <radius>.082</radius>
                        <length>.06</length>
                    </cylinder>
                </geometry>
            </collision>

            <visual name="chassis_visual">
                <pose>0 0 .04 0 0 1.5708</pose>
                <geometry>
                    <mesh>
                        <uri>model://romi/chassis.dae</uri>
                    </mesh>
                </geometry>
            </visual>

            <collision name="rear_caster_collision">
                <pose>-.06 0 .016 0 0 0</pose>
                <geometry>
                    <sphere>
                        <radius>.0127</radius>
                    </sphere>
                </geometry>

                <!-- zero friction -->
                <surface>
                    <friction>
                        <ode>
                            <mu>0</mu>
                            <mu2>0</mu2>
                            <slip1>1.0</slip1>
                            <slip2>1.0</slip2>
                        </ode>
                    </friction>
                </surface>
            </collision>

            <visual name="rear_caster_visual">
                <pose>-.06 0 .0155 0 0 0</pose>
                <geometry>
                    <sphere>
                        <radius>.0127</radius>
                    </sphere>
                </geometry>
            </visual>

            <collision name="front_caster_collision">
                <pose>.06 0 .016 0 0 0</pose>
                <geometry>
                    <sphere>
                        <radius>.0127</radius>
                    </sphere>
                </geometry>

                <!-- zero friction -->
                <surface>
                    <friction>
                        <ode>
                            <mu>0</mu>
                            <mu2>0</mu2>
                            <slip1>1.0</slip1>
                            <slip2>1.0</slip2>
                        </ode>
                    </friction>
                </surface>
            </collision>

            <visual name="front_caster_visual">
                <pose>.06 0 .0155 0 0 0</pose>
                <geometry>
                    <sphere>
                        <radius>.0127</radius>
                    </sphere>
                </geometry>
            </visual>

            <!-->
                The base and wheels can be approximated by solid cylinders
                or radius R, mass M, and height H. This cylinder is standing
                "straight". (Wheels need to be rotated)

                Iz    = M*R^2/2
                Ix=Iy = M/12*(3*R^2 + H^2)
            <-->
            <inertial>
                <pose>0 0 .04 0 0 0</pose>
                <mass>0.2</mass>
                <inertia>
                    <ixx>0.000363</ixx>
                    <iyy>0.000363</iyy>
                    <izz>0.001345</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>

            <sensor type="gpu_ray" name="dist_sensor_0">
                <pose>.06 0 .033 0 0 0</pose>
                <visualize>true</visualize>
                <update_rate>15</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>3</samples>
                            <resolution>1</resolution>
                            <min_angle>-.01</min_angle>
                            <max_angle>.01</max_angle>
                        </horizontal>
                    </scan>
                    <range>
                        <min>0.01</min>
                        <max>2.0</max>
                        <resolution>0.005</resolution>
                    </range>
                </ray>
            </sensor>

            <sensor type="gpu_ray" name="dist_sensor_n45">
                <pose>.06 0 .033 0 0 .7854</pose>
                <visualize>true</visualize>
                <update_rate>15</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>3</samples>
                            <resolution>1</resolution>
                            <min_angle>-.01</min_angle>
                            <max_angle>.01</max_angle>
                        </horizontal>
                    </scan>
                    <range>
                        <min>0.01</min>
                        <max>2.0</max>
                        <resolution>0.005</resolution>
                    </range>
                </ray>
            </sensor>

            <sensor type="gpu_ray" name="dist_sensor_p45">
                <pose>.06 0 .033 0 0 -.7854</pose>
                <visualize>true</visualize>
                <update_rate>15</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>3</samples>
                            <resolution>1</resolution>
                            <min_angle>-.01</min_angle>
                            <max_angle>.01</max_angle>
                        </horizontal>
                    </scan>
                    <range>
                        <min>0.01</min>
                        <max>2.0</max>
                        <resolution>0.005</resolution>
                    </range>
                </ray>
            </sensor>


        </link>

        <link name="left_wheel">
            <collision name="left_wheel_collision">
                <pose>.0034 .069 .038 1.5708 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>.035</radius>
                        <length>.012</length>
                    </cylinder>
                </geometry>
            </collision>

            <visual name="left_wheel_visual">
                <pose>.0034 .069 .038 0 0 1.5708</pose>
                <geometry>
                    <mesh>
                        <uri>model://romi/wheel.dae</uri>
                    </mesh>
                </geometry>
            </visual>

            <inertial>
                <pose>.0034 .069 .038 0 0 1.5708</pose>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.000336</ixx>
                    <iyy>0.000091</iyy>
                    <izz>0.000091</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
        </link>

        <link name="right_wheel">
            <collision name="right_wheel_collision">
                <pose>.0034 -.069 .038 1.5708 0 0</pose>
                <geometry>
                    <cylinder>
                        <radius>.035</radius>
                        <length>.012</length>
                    </cylinder>
                </geometry>
            </collision>

            <visual name="right_wheel_visual">
                <pose>.0034 -.069 .038 0 0 -1.5708</pose>
                <geometry>
                    <mesh>
                        <uri>model://romi/wheel.dae</uri>
                    </mesh>
                </geometry>
            </visual>

            <inertial>
                <pose>.0034 -.069 .038 1.5708 0 0</pose>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.000336</ixx>
                    <iyy>0.000091</iyy>
                    <izz>0.000091</izz>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyz>0</iyz>
                </inertia>
            </inertial>
        </link>

        <joint type="revolute" name="left_wheel_joint">
            <pose>.0034 .067 .038 0 0 1.5708</pose>
            <parent>chassis</parent>
            <child>left_wheel</child>
            <axis>
                <xyz>1 0 0</xyz>
                <limit>
                    <lower>-10000000000000000</lower>
                    <upper>10000000000000000</upper>
                </limit>
            </axis>
        </joint>

        <joint type="revolute" name="right_wheel_joint">
            <pose>.0034 -.067 .038 0 0 1.5708</pose>
            <parent>chassis</parent>
            <child>right_wheel</child>
            <axis>
                <xyz>-1 0 0</xyz>
                <limit>
                    <lower>-10000000000000000</lower>
                    <upper>10000000000000000</upper>
                </limit>
            </axis>
        </joint>

    </model>
</sdf>